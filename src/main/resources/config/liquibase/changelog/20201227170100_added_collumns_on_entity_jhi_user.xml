<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd
                        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

    
    <!--
        Added the collmns on User.
    -->
  
  <changeSet id="20201227170100-1" author="john">
    <addColumn tableName="jhi_user">
      <column name="data_nascimento" type="date">
          <constraints nullable="true" />
      </column>
      <column name="cpf" type="varchar(255)">
          <constraints nullable="true" />
      </column>
      <column name="fone" type="varchar(16)">
          <constraints nullable="true" />
      </column>
      <column name="celular" type="varchar(16)">
          <constraints nullable="true" />
      </column>
      <column name="logradouro" type="varchar(50)">
          <constraints nullable="true" />
      </column>
      <column name="numero" type="varchar(6)">
          <constraints nullable="true" />
      </column>
      <column name="complemento" type="varchar(30)">
          <constraints nullable="true" />
      </column>
      <column name="bairro" type="varchar(50)">
          <constraints nullable="true" />
      </column>
      <column name="cidade" type="varchar(50)">
          <constraints nullable="true" />
      </column>
      <column name="cep" type="varchar(8)">
          <constraints nullable="true" />
      </column>
      <column name="uf" type="varchar(2)">
          <constraints nullable="true" />
      </column>
    </addColumn>
  </changeSet>

</databaseChangeLog>